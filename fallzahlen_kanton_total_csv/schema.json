{
  "$schema": "http://json-schema.org/schema#",
  "definitions": {
    "optionalInteger": {
      "anyOf": [{ "type": "integer" }, { "type": "string", "maxLength": 0 }]
    }
  },
  "properties": {
    "date": {
      "type": "string",
      "format": "date"
    },
    "time": {
      "anyOf": [
        {
          "type": "string",
          "pattern": "^([0-1]?[0-9]|2[0-3]):[0-5][0-9]$",
          "$comment": "This should really be `format: time` but the currently used format doesn't match"
        },
        { "type": "string", "maxLength": 0 }
      ]
    },
    "abbreviation_canton_and_fl": {
      "type": "string",
      "enum": [
        "AG",
        "AI",
        "AR",
        "BE",
        "BL",
        "BS",
        "FR",
        "GE",
        "GL",
        "GR",
        "JU",
        "LU",
        "NE",
        "NW",
        "OW",
        "SG",
        "SH",
        "SZ",
        "TG",
        "TI",
        "UR",
        "VD",
        "VS",
        "ZG",
        "ZH",
        "FL"
      ]
    },
    "ncumul_tested": {
      "$ref": "#/definitions/optionalInteger"
    },
    "ncumul_conf": {
      "$ref": "#/definitions/optionalInteger"
    },
    "ncumul_hosp": {
      "$ref": "#/definitions/optionalInteger"
    },
    "ncumul_ICU": {
      "$ref": "#/definitions/optionalInteger"
    },
    "ncumul_vent": {
      "$ref": "#/definitions/optionalInteger"
    },
    "ncumul_released": {
      "$ref": "#/definitions/optionalInteger"
    },
    "ncumul_deceased": {
      "$ref": "#/definitions/optionalInteger"
    },
    "source": {
      "anyOf": [
        {
          "type": "string",
          "format": "uri"
        },
        {
          "type": "string",
          "maxLength": 0
        }
      ]
    }
  },
  "required": [
    "date",
    "time",
    "abbreviation_canton_and_fl",
    "ncumul_tested",
    "ncumul_conf",
    "ncumul_hosp",
    "ncumul_ICU",
    "ncumul_vent",
    "ncumul_released",
    "ncumul_deceased",
    "source"
  ]
}
